      "source": [
        "L1 = [1, 2, 2, 3, 2, 3, 4, 5]\n",
        "most_repeated = None\n",
        "max_count = 0\n",
        "\n",
        "for num in L1:\n",
        "    count = L1.count(num)\n",
        "    if count > max_count:\n",
        "        max_count = count\n",
        "        most_repeated = num\n",
        "\n",
        "print(\"The most repetitive number is:\", most_repeated)"
      ]
